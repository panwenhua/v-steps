{
  "name": "vue-steps",
  "version": "0.1.0",
  "author": "panwenhua",
  "files": [
    "lib"
  ],
  "scripts": {
    "http": "http-server -a 0.0.0.0 -p 9999 -c-1 ./lib",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build  --dest ./docs",
    "build-bundle": "vue-cli-service build  --dest ./lib --target lib --name steps ./src/components/index.js",
    "lint": "vue-cli-service lint"
  },
  "main": "./lib/steps.common.js",
  "dependencies": {
    "core-js": "^2.6.5",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.1.1",
    "@vue/cli-plugin-eslint": "^3.1.1",
    "@vue/cli-service": "^3.1.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.5.21",
    "http-server": "^0.11.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
